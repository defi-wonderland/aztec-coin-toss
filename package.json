{
  "name": "private-oracle",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "compile": "aztec-cli compile . -o './src/app/src/abis/cointoss' -i './src/app/src/abis/cointoss' -ts './src/app/src/abis/cointoss'",
    "test": "yarn compile && NODE_NO_WARNINGS=1 node --experimental-vm-modules $(yarn bin jest) --runInBand --passWithNoTests --testTimeout=15000",
    "start:dev": "yarn --cwd ./src/app start:dev",
    "postinstall": "yarn --cwd ./src/app"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testRegex": "./src/.*\\.test\\.ts$",
    "rootDir": "./src"
  },
  "dependencies": {
    "@aztec/aztec-sandbox": "0.15.1",
    "@aztec/aztec.js": "0.15.1",
    "@aztec/cli": "0.15.1",
    "@aztec/foundation": "0.15.1",
    "@aztec/types": "0.15.1",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
